{
    "name": "wiz/resque-bundle",
    "description": "The WIZ resque bundle provides integration of php-resque to Symfony2.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "JackLam",
            "email": "jack@wizmacau.com",
            "homepage": "https://lamjack.github.io"
        }
    ],
    "require": {
        "symfony/symfony": "3.4.*",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Wiz\\ResqueBundle\\": ""
        }
    },
    "bin": ["bin/resque", "bin/resque-scheduler"],
    "extra": {
        "branch-alias": {
            "dev-master": "3.4.x-dev"
        }
    }
}
